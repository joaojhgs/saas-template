{
  "name": "sass-template",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev --turbo",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:i18n": "eslint --ext .json --format node_modules/eslint-plugin-i18n-json/formatter.js src/locale/messages",
    "format": "prettier --write \"**/*.{js,ts,tsx,mdx}\" --cache",
    "check": "prettier --check \"**/*.{js,ts,tsx,mdx}\" --cache",
    "prepare": "husky",
    "commit": "git-cz",
    "supabase:generate": "supabase gen types typescript --local --schema public > src/schemas/supabase.ts ",
    "supabase": "supabase"
  },
  "dependencies": {
    "@ant-design/cssinjs": "^1.20.0",
    "@supabase/ssr": "^0.3.0",
    "@supabase/supabase-js": "^2.42.7",
    "@tanstack/react-query": "^5.35.1",
    "antd": "^5.16.4",
    "antd-img-crop": "^4.22.0",
    "antd-zod": "^5.1.0",
    "client-only": "^0.0.1",
    "clsx": "^1.2.1",
    "dayjs": "^1.11.10",
    "jiti": "^1.21.0",
    "lodash": "^4.17.21",
    "lucide-react": "^0.244.0",
    "next": "14.2.2",
    "next-intl": "3.11.3",
    "next-themes": "^0.3.0",
    "react": "18.2.0",
    "react-big-calendar": "1.12.2",
    "react-dom": "18.2.0",
    "server-only": "^0.0.1",
    "tailwind-merge": "^1.14.0",
    "type-fest": "^4.18.2",
    "zod": "^3.23.0",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.2",
    "@commitlint/config-conventional": "^19.2.2",
    "@t3-oss/env-nextjs": "^0.10.0",
    "@tanstack/react-query-devtools": "^5.35.1",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/lodash": "^4.17.4",
    "@types/node": "18.13.0",
    "@types/react": "18.0.27",
    "@types/react-big-calendar": "1.8.9",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "6.0.0",
    "autoprefixer": "^10.4.19",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.2",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-i18n-json": "^4.0.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-tailwindcss": "^3.15.1",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "supabase": ">=1.8.1",
    "tailwindcss": "^3.4.3",
    "typescript": "5.1.3",
    "typescript-eslint": "^7.7.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
