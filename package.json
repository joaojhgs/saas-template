{
  "name": "sass-template",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev --turbopack",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "lint:i18n": "eslint --ext .json --format node_modules/eslint-plugin-i18n-json/formatter.js src/locale/messages",
    "format": "prettier --write \"**/*.{js,ts,tsx,mdx}\" --cache",
    "check": "prettier --check \"**/*.{js,ts,tsx,mdx}\" --cache",
    "prepare": "husky",
    "commit": "git-cz",
    "supabase:generate": "supabase gen types typescript --local --schema public > src/schemas/supabase.ts ",
    "supabase": "supabase"
  },
  "dependencies": {
    "@ant-design/cssinjs": "^2.0.1",
    "@ant-design/nextjs-registry": "^1.3.0",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.86.0",
    "@tanstack/react-query": "^5.90.11",
    "@trpc/client": "^11.7.2",
    "@trpc/server": "^11.7.2",
    "@trpc/tanstack-react-query": "^11.7.2",
    "@vercel/flags": "^2.5.1",
    "antd": "^6.0.1",
    "antd-img-crop": "^4.22.0",
    "antd-zod": "^6.1.0",
    "client-only": "^0.0.1",
    "clsx": "^1.2.1",
    "dayjs": "^1.11.10",
    "jiti": "^1.21.0",
    "lodash": "^4.17.21",
    "lucide-react": "^0.555.0",
    "next": "16.0.7",
    "next-intl": "4.5.8",
    "next-themes": "^0.4.6",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "server-only": "^0.0.1",
    "superjson": "^2.2.6",
    "tailwind-merge": "^1.14.0",
    "type-fest": "^4.18.2",
    "zod": "^3.23.8",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.2",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.1",
    "@t3-oss/env-nextjs": "^0.11.1",
    "@tanstack/react-query-devtools": "^5.91.1",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/lodash": "^4.17.4",
    "@types/node": "18.13.0",
    "@types/react": "19.2.7",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "6.0.0",
    "autoprefixer": "^10.4.19",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9",
    "eslint-config-next": "16.0.7",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-i18n-json": "^4.0.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-tailwindcss": "^3.15.1",
    "husky": "^9.0.11",
    "jsonc-eslint-parser": "^2.4.1",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "supabase": ">=1.8.1",
    "tailwindcss": "^3.4.3",
    "typescript": "5.1.3",
    "typescript-eslint": "^7.7.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
